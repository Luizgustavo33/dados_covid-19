CREATE TABLE covid_19 (
  idcovid_19 INTEGER   NOT NULL ,
  city VARCHAR(100)    ,
  city_ibge_code INT    ,
  epidemiological_week   INT    ,
  estimated_population   INT    ,
  estimated_population_2019   INT    ,
  is_last   VARCHAR(15)    ,
  state VARCHAR(4)    ,
  is_repeated   DATE    ,
  last_available_confirmed   INT    ,
  deaths VARCHAR(100)    ,
  last_available_confirmed_per_100k_inhabitants   DOUBLE    ,
  last_available_date   DATE      ,
PRIMARY KEY(idcovid_19));





